<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travelhacking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".directflightsMonths"></activity>
        <activity android:name=".Months" />
        <activity android:name=".Itinerary" />
        <activity android:name=".directflights" />
        <activity android:name=".Airportrow" />
        <activity android:name=".AirportList" />
        <activity android:name=".Edit_program" />
        <activity android:name=".edit_cc" />
        <activity android:name=".Add_Loyalty" />
        <activity android:name=".Add_Card" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>